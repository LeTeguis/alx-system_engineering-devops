echo '#!/bin/bash' >> 0x01-shell_permissions/12-directory_permissions
echo 'mkdir -m 751 0x01-shell_permissions/my_dir' >> 0x01-shell_permissions/12-directory_permissions
chmod u+x 0x01-shell_permissions/12-directory_permissions
git add .
git commit -m "directory permissions"
git push
clear
vi script_me
./script_me
