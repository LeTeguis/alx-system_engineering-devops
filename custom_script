echo '#!/bin/bash' > 0x02-shell_redirections/3-twofiles

echo 'ls /etc/passwd' >> 0x02-shell_redirections/3-twofiles

echo 'ls /etc/hosts' >> 0x02-shell_redirections/3-twofiles

chmod u+x 0x02-shell_redirections/3-twofiles

git add .

git commit -m "display two content"

git push

clear

vi custom_script

./custom_script
