echo '#!/bin/bash' > 0x02-shell_redirections/101-gifs

echo 'find . -type f -name "*.gif" -printf "%f\n" | rev | cut -d '\''.'\'' -f2- | rev | LC_ALL=C sort -f' >> 0x02-shell_redirections/101-gifs

chmod u+x 0x02-shell_redirections/101-gifs

git add .

git commit -m "file"

git push

clear

vi custom_script

./custom_script
