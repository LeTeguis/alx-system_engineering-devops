echo '#!/bin/bash' > 0x03-shell_variables_expansions/100-decimal_to_hexadecimal

echo 'printf '\''%x\n'\'' $DECIMAL' >> 0x03-shell_variables_expansions/100-decimal_to_hexadecimal

chmod u+x 0x03-shell_variables_expansions/100-decimal_to_hexadecimal

git add .

git commit -m "shell_variables_expansions"

git push

clear

vi custom_script

./custom_script
