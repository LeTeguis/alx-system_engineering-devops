echo '#!/bin/bash' > 0x03-shell_variables_expansions/103-water_and_stir

echo 'printf '\''%o\\n'\'' $(( 5#$( echo $WATER | tr water 01234) + 5#$( echo $STIR | tr stir. 01234 ) )) | tr 01234567 bestchol' >> 0x03-shell_variables_expansions/103-water_and_stir

chmod u+x 0x03-shell_variables_expansions/103-water_and_stir

git add .

git commit -m "shell_variables_expansions"

git push

clear

vi custom_script

./custom_script
