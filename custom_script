echo '#!/bin/bash' > 0x03-shell_variables_expansions/9-divide_and_rule

echo 'echo $(( $POWER / $DIVIDE ))' >> 0x03-shell_variables_expansions/9-divide_and_rule

chmod u+x 0x03-shell_variables_expansions/9-divide_and_rule

