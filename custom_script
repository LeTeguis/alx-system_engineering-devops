echo '#!/bin/bash' > 0x02-shell_redirections/22-users_and_homes

echo 'cat /etc/passwd | sort -k1 | cut -d '\'':'\'' -f1,6' >> 0x02-shell_redirections/22-users_and_homes

chmod u+x 0x02-shell_redirections/22-users_and_homes

git add .

git commit -m "file"

git push

clear

vi custom_script

./custom_script
